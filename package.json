{
  "name": "backpackflow",
  "version": "1.0.0",
  "description": "A config-driven LLM framework built on top of PocketFlow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./pocketflow": {
      "types": "./dist/pocketflow.d.ts",
      "require": "./dist/pocketflow.js",
      "import": "./dist/pocketflow.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "require": "./dist/providers/index.js",
      "import": "./dist/providers/index.js"
    },
    "./nodes": {
      "types": "./dist/nodes/index.d.ts",
      "require": "./dist/nodes/index.js",
      "import": "./dist/nodes/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "ts-node",
    "test": "jest",
    "test:watch": "jest --watch",
    "example": "ts-node examples/explore-pocketflow.ts"
  },
  "keywords": [
    "llm",
    "workflow",
    "agents",
    "pocketflow",
    "config-driven"
  ],
  "author": "",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.19",
    "ai": "^5.0.22",
    "dotenv": "^17.2.1",
    "openai": "^5.15.0",
    "readline": "^1.3.0"
  }
}
